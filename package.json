{
  "name": "cognitive-distortions",
  "type": "module",
  "version": "0.1.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:open": "npm run start -- --open",
    "deploy": "git checkout -B gh-pages && npm run build:prod && git add . && git commit -m 'uploading artifacts' && git push -f && git checkout main",
    "build:prod": "tsx scripts/sw-set-project-version.ts && npm run build -- --base-href /cognitive-distortions/ && node scripts/fix-output.js",
    "build:serve": "tsx scripts/sw-set-project-version.ts && npm run build && npx http-server -p 8080 -c-1 dist/cognitive-distortions/browser",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.1",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.1",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/service-worker": "^19.0.0",
    "angular-animations": "^0.11.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "ngx-mat-timepicker": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular/cli": "^19.0.2",
    "@angular/compiler-cli": "^19.0.0",
    "@types/hammerjs": "^2.0.46",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.14",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2"
  }
}
